{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Range Tranched Stepper CSV Schema",
  "description": "Schema for tranched stepper vesting with absolute timestamps",
  "type": "object",
  "properties": {
    "address": {
      "$ref": "../../common.schema.json#/$defs/address"
    },
    "amount": {
      "$ref": "../../common.schema.json#/$defs/amount"
    },
    "startAt": {
      "$ref": "../../common.schema.json#/$defs/timestamp"
    },
    "endAt": {
      "$ref": "../../common.schema.json#/$defs/timestamp"
    },
    "stepCount": {
      "$ref": "../../common.schema.json#/$defs/positiveInteger"
    }
  },
  "required": ["address", "amount", "startAt", "endAt", "stepCount"],
  "additionalProperties": false
}
