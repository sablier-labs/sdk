{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Range Dynamic Double Cliff CSV Schema",
  "description": "Schema for dynamic double cliff vesting with absolute timestamps",
  "type": "object",
  "properties": {
    "address": {
      "$ref": "../../common.schema.json#/$defs/address"
    },
    "amount": {
      "$ref": "../../common.schema.json#/$defs/amount"
    },
    "startAt": {
      "$ref": "../../common.schema.json#/$defs/timestamp"
    },
    "endAt": {
      "$ref": "../../common.schema.json#/$defs/timestamp"
    },
    "firstUnlockAmount": {
      "$ref": "../../common.schema.json#/$defs/amount"
    },
    "firstUnlockAt": {
      "$ref": "../../common.schema.json#/$defs/timestamp"
    },
    "secondUnlockAmount": {
      "$ref": "../../common.schema.json#/$defs/amount"
    },
    "secondUnlockAt": {
      "$ref": "../../common.schema.json#/$defs/timestamp"
    }
  },
  "required": [
    "address",
    "amount",
    "startAt",
    "endAt",
    "firstUnlockAmount",
    "firstUnlockAt",
    "secondUnlockAmount",
    "secondUnlockAt"
  ],
  "additionalProperties": false
}
