{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Range Linear Unlock Cliff CSV Schema",
  "description": "Schema for linear unlock cliff vesting with absolute timestamps",
  "type": "object",
  "properties": {
    "address": {
      "$ref": "../../common.schema.json#/$defs/address"
    },
    "amount": {
      "$ref": "../../common.schema.json#/$defs/amount"
    },
    "startAt": {
      "$ref": "../../common.schema.json#/$defs/timestamp"
    },
    "endAt": {
      "$ref": "../../common.schema.json#/$defs/timestamp"
    },
    "unlockAmount": {
      "$ref": "../../common.schema.json#/$defs/amount"
    },
    "cliffAmount": {
      "$ref": "../../common.schema.json#/$defs/amount"
    },
    "cliffAt": {
      "$ref": "../../common.schema.json#/$defs/timestamp"
    }
  },
  "required": ["address", "amount", "startAt", "endAt", "unlockAmount", "cliffAmount", "cliffAt"],
  "additionalProperties": false
}
